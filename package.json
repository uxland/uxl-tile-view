  },
  "bugs": {
    "url": "https://github.com/uxland/uxl-tile-view/issues"
  },
  "dependencies": {
    "@polymer/iron-icons": "^3.0.1",
    "lit-element": "^2.0.0-rc.3",
    "@polymer/paper-icon-button": "^3.0.1",
    "@polymer/polymer": "^3.0.0",
    "@uxland/uxl-polymer2-ts": "^1.0.11"
  },
  "devDependencies": {
    "@polymer/iron-demo-helpers": "^3.0.0-pre.19",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.2",
    "@types/redux-mock-store": "^1.0.0",
    "@types/sinon": "^5.0.1",
    "@types/webdriverio": "^4.10.3",
    "@webcomponents/custom-elements": "^1.2.0",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "chai": "^4.1.2",
    "dts-generator": "^2.1.0",
    "esm": "^3.0.72",
    "gulp": "^3.9.1",
    "gulp-bump": "^3.1.1",
    "gulp-run": "^1.7.1",
    "jsdom": "^11.12.0",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.3",
    "mutation-observer": "^1.0.3",
    "ncp": "^2.0.0",
    "nsp": "^2.6.3",
    "raf": "^3.4.0",
    "redux-mock-store": "git+https://github.com/dpinart/redux-mock-store.git",
    "require-dir": "^0.3.2",
    "rimraf": "^2.6.2",
    "run-sequence": "^2.1.0",
    "semantic-release": "^15.9.12",
    "sinon": "^6.0.0",
    "travis-deploy-once": "^5.0.2",
    "typescript": "^3.2.4",
    "wct-browser-legacy": "^1.0.0",
    "wdio-dot-reporter": "0.0.10",
    "wdio-mocha-framework": "^0.6.3",
    "webdriverio": "^4.13.2",
    "selenium-standalone": "^6.15.1",
    "wdio-allure-reporter": "^0.7.0",
    "allure-commandline": "^2.7.0",
    "wdio-selenium-standalone-service": "0.0.10",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "common-js": "^0.3.8",
    "tslib": "^1.9.3",
    "wc-sass-render": "^1.1.1"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json || echo 'Skip common compile errors' ",
    "test:ui:watch": "nodemon --watch test --watch src -x \"polymer test --config-file wct.config.json\"",
    "test:ui": "polymer test --config-file wct.config.json",
    "test:unit": "npm run build && node --experimental-modules node_modules/mocha/bin/_mocha -r esm --ui tdd --reporter mochawesome test/unit",
    "dist": "tsc --p gulp/tsconfig-dist.json || echo 'Skip common compile errors' && gulp generate-d-ts",
    "tdd": "node --experimental-modules node_modules/mocha/bin/_mocha --watch -r esm --ui tdd --reporter mochawesome test/unit",
    "prepublishOnly": "if [ -d dist ]; then ncp dist/. . && rimraf dist/; fi",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "pretest:e2e": "rimraf allure-results",
    "test:e2e": "wdio",
    "test:generate-report": "allure generate allure-results --clean -o allure-report && allure open",
    "posttest:e2e": "npm run test:generate-report",
    "compile-sass": "sass-render ./src/**/[!^_]*.scss --suffix .ts -t src/utilities/sass-template.js && npm run build"
  },
  "publishConfig": {
    "access": "public"
  }
}
